{
	"name": "vetro",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"proxy": "http://localhost:3000",
	"scripts": {
		"init:check": "bin/init_check",
		"check:dockerignore": "rsync -avn . /dev/shm --exclude-from .dockerignore | less",
		"dev:test": "bun dev:postgres && bun test",
		"dev:migrate": "bin/migrate_dev",
		"dev:postgres": "docker context use default && docker compose -f config/postgres_dev/compose.yaml up -d",
		"dev:docker": "docker context use default && docker compose -f config/dev/compose.yaml up -d --build",
		"dev:docker:stop": "docker context use default && docker compose -f config/dev/compose.yaml down",
		"dev:vite": "vite",
		"dev:lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"dev:preview": "vite preview",
		"prod:test": "bunx prisma generate && bun test",
		"prod:build": "tsc && vite build",
		"prod:deploy": "docker context use remote && docker compose -f config/production/compose.yaml up -d --build",
		"prod:entry": "bin/prod_entry"
	},
	"dependencies": {
		"@apollo/client": "^3.9.5",
		"@prisma/client": "^5.10.2",
		"apollo": "^2.34.0",
		"express": "^4.18.3",
		"postgraphile": "^4.13.0",
		"prisma": "^5.10.2",
		"react": "^18.2.0",
		"react-digraph": "^9.1.2",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.22.2",
		"styled-components": "^6.1.8",
		"graphiql": "^3.1.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.5.3",
		"@types/express": "^4.17.21",
		"@types/react": "^18.2.56",
		"@types/react-dom": "^18.2.19",
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"@typescript-eslint/parser": "^7.0.2",
		"@vitejs/plugin-react": "^4.2.1",
		"cors": "^2.8.5",
		"eslint": "^8.57.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"typescript": "^5.2.2",
		"vite": "^5.1.4"
	}
}
