{
	"name": "vetro",
	"private": true,
	"version": "0.0.9",
	"type": "module",
	"proxy": "http://localhost:3000",
	"scripts": {
		"init:check": "bin/init_check",
		"check:dockerignore": "rsync -avn . /dev/shm --exclude-from .dockerignore | less",
		"dev:test": "bun dev:postgres && bun test",
		"dev:migrate": "bin/migrate_dev",
		"dev:postgres": "docker context use default && docker compose -f config/postgres_dev/compose.yaml up -d",
		"dev:docker": "docker context use default && docker compose -f config/dev/compose.yaml up -d --build",
		"dev:docker:stop": "docker context use default && docker compose -f config/dev/compose.yaml down",
		"dev:vite": "vite",
		"dev:lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"dev:preview": "vite preview",
		"dev:codegen": "graphql-codegen --config codegen.ts",
		"prod:test": "bunx prisma generate && bun test",
		"prod:deploy": "docker context use remote && docker compose -f config/production/compose.yaml pull && docker compose -f config/production/compose.yaml up -d --build",
		"prod:stop": "docker context use remote && docker compose -f config/production/compose.yaml down",
		"prod:entry": "bin/prod_entry"
	},
	"dependencies": {
		"@apollo/client": "^3.9.5",
		"@graphql-typed-document-node/core": "^3.2.0",
		"@prisma/client": "^5.10.2",
		"@xstate/react": "^4.1.0",
		"apollo": "^2.34.0",
		"date-fns": "2.30.0",
		"date-fns-tz": "^2.0.0",
		"express": "^4.18.3",
		"graphiql": "^3.1.1",
		"graphql": "^16.8.1",
		"localforage": "^1.10.0",
		"lodash": "^4.17.21",
		"postgraphile": "^5.0.0-beta.21",
		"prisma": "^5.10.2",
		"react": "^18.2.0",
		"react-digraph": "^9.1.2",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.51.0",
		"react-router-dom": "^6.22.2",
		"react-use": "^17.5.0",
		"styled-components": "^6.1.8",
		"uuid": "^9.0.1",
		"xstate": "^5.9.1"
	},
	"devDependencies": {
		"@0no-co/graphqlsp": "^1.4.2",
		"@biomejs/biome": "^1.5.3",
		"@graphql-codegen/cli": "^5.0.2",
		"@graphql-codegen/client-preset": "^4.2.4",
		"@graphql-codegen/introspection": "^4.0.3",
		"@parcel/watcher": "^2.4.1",
		"@types/express": "^4.17.21",
		"@types/lodash": "^4.14.202",
		"@types/react": "^18.2.61",
		"@types/react-dom": "^18.2.19",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"@typescript-eslint/parser": "^7.0.2",
		"@vitejs/plugin-react": "^4.2.1",
		"cors": "^2.8.5",
		"eslint": "^8.57.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"ts-node": "^10.9.2",
		"typescript": "^5.2.2",
		"vite": "^5.1.4"
	},
	"resolutions": {
		"grafast": "^0.1.1-beta.6",
		"graphql": "^16.8.1"
	}
}
